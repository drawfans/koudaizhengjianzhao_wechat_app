.van-toast {
    position: fixed;
    top: 50%;
    left: 50%;
    display: flex;
    color: #fff;
    max-width: 70%;
    font-size: 28rpx;
    line-height: 40rpx;
    border-radius: 8rpx;
    word-break: break-all;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    box-sizing: content-box;
    transform: translate3d(-50%,-50%,0);
    background-color: rgba(50,50,51,0.88);
    width: fit-content;
    white-space: pre-wrap;
}

.van-toast--unclickable ._view,.van-toast--unclickable ._text {
    pointer-events: none;
}

.van-toast--unclickable view,.van-toast--unclickable text {
