.par-tabbar {
    display: flex;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    z-index: 10;
}

.par-tabbar-item {
    display: flex;
    justify-content: center;
    flex: 1;
    background: #fff;
}

.par-tabbar-item .par-tabbar-content {
    position: relative;
    display: flex;
    flex-flow: column;
    justify-content: center;
    align-items: center;
}

.par-tabbar-item .par-info {
    position: absolute;
    right: 0;
    top: -16rpx;
    color: #fff;
    font-size: 24rpx;
    font-weight: 500;
    font-family: PingFang SC,Helvetica Neue,Arial,sans-serif;
    text-align: center;
    box-sizing: border-box;
    padding: 0 6rpx;
    min-width: 32rpx;
    line-height: 28rpx;
    border: 2rpx solid #fff;
    border-radius: 32rpx;
    background-color: #608dff;
    transform: translateX(50%);
    transform-origin: 100%;
    z-index: 1;
}

.par-tabbar-item .par-infos {
    position: absolute;
    right: 0;
    top: 0;
    color: #fff;
    font-size: 24rpx;
    font-weight: 500;
    font-family: PingFang SC,Helvetica Neue,Arial,sans-serif;
    text-align: center;
    box-sizing: border-box;
    padding: 0 6rpx;
    min-width: 32rpx;
    line-height: 28rpx;
    border: 2rpx solid #fff;
    border-radius: 32rpx;
    background-color: #f53303;
    transform: translateX(50%);
    transform-origin: 100%;
    white-space: nowrap;
    right: -40rpx;
    z-index: 1;
}

.tab-bar .par-info {
    top: 0;
    right: auto;
    left: 50%;
